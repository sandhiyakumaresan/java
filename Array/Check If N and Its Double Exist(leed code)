import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new  Scanner(System.in);
		int n =sc.nextInt();
		int a[]=new int[n];
		for(int i=0;i<n;i++){
		    a[i]=sc.nextInt();
		}
		for(int i=0;i<n;i++){
		    for(int j=0;j<n;j++){
		        if(i!=j&&(a[i]==2*a[j]||a[j]==2*a[i])){
		            System.out.print("true");
		            return;
		        }
		    }
		}
		System.out.print("false");
	}
}
input:[10,2,5,6] 
output:true
