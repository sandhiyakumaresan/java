import java.util.*;
public class Main
{
	public static void main(String[] args) {
	 Scanner sc = new Scanner (System.in);
	 int n = sc.nextInt();
	 int a[]=new int[n];
	 for(int i=0;i<n;i++){
	     a[i]=sc.nextInt();
	 }
	 int b=0;
	 for(int i =0;i<n;i++){
	     if(a[i]!=0){
	         a[b]=a[i];
	         b++;
	     }
	 }
	     while(b<n){
	         a[b]=0;
	         b++;
	     }
	   for(int i=0;i<n;i++){
	       System.out.print(a[i]);
	   }
   }
}

input:3 0 2 0 1
output:3 2 1 0 0 
