import java.util.*;
public class Main {
    public static void main (String args[]){
        Scanner sc =new Scanner (System.in);
        int n= sc.nextInt();
        int l =n%10;
        if(l==7||n%7==0){
            System.out.print("buzzy number");
        }
        else{
            System.out.print("not a buzzy number");
        }
    }  
}
