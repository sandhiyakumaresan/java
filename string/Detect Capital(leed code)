import java.util.*;
public class Main
{
	public static void main(String[] args) {
	 Scanner sc =new Scanner(System.in);
	 String s =sc.nextLine();
	 int l =s.length();
	 int f=1;
	 for(int i =0;i<l;i++){
	     char ch=s.charAt(i);
	     if(ch>='A'||ch<='Z'){
	         f=0;
	         break;
	     }
	   }
	   if(f==1){
	       System.out.print("true");
	   }
	   else{
	       System.out.print("false");
	   }
	}
}
